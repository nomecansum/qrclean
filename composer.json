{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ext-imagick": "*",
        "24slides/laravel-saml2": "^2.0",
        "barryvdh/laravel-async-queue": "^0.7.5",
        "barryvdh/laravel-snappy": "^1.0",
        "berkayk/onesignal-laravel": "^1.0",
        "darkaonline/l5-swagger": "^8.3",
        "dragonmantank/cron-expression": "^3.3",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.5",
        "jenssegers/agent": "^2.6",
        "laracasts/flash": "^3.2",
        "laravel/fortify": "^1.13",
        "laravel/framework": "^9.11",
        "laravel/passport": "^10.4",
        "laravel/sanctum": "^2.14.1",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.0",
        "league/flysystem-aws-s3-v3": "^3.2",
        "maatwebsite/excel": "^3.1",
        "opcodesio/log-viewer": "^1.2",
        "phpoffice/phpspreadsheet": "^1.21",
        "riverskies/laravel-mobile-detect": "^1.3",
        "sebdesign/blade-sql-formatter": "^0.1.1",
        "shahonseven/php-color-hash": "^1.0",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "socialiteproviders/microsoft": "^4.1",
        "spatie/icalendar-generator": "^2.1",
        "spatie/laravel-ignition": "^1.3",
        "symfony/error-handler": "^6.1",
        "tremby/laravel-git-version": "^1.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "crestapps/laravel-code-generator": "^2.4",
        "fzaninotto/faker": "^1.9.1",
        "heroku/heroku-buildpack-php": "dev-master",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "bootstrap/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [ 
            "php artisan clear-compiled",
            "chmod -R 777 storage", 
            "php artisan passport:keys"
        ]
    }
}
